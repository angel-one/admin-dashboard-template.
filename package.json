{
	"name": "admin_dashboard_template",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"dev:uat": "cp .env.uat .env && vite dev",
		"dev:cug": "cp .env.cug .env && vite dev",
		"build": "vite build",
		"build:uat": "cp .env.uat .env && vite build",
		"build:cug": "cp .env.cug .env && vite build",
		"build:prod": "cp .env.prod .env && vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@playwright/test": "^1.31.2",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.13.0",
		"@types/pg": "^8.6.6",
		"@typescript-eslint/eslint-plugin": "^5.56.0",
		"@typescript-eslint/parser": "^5.56.0",
		"autoprefixer": "^10.4.14",
		"cookie-es": "^0.5.0",
		"eslint": "^8.36.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-svelte3": "^4.0.0",
		"flatpickr": "^4.6.13",
		"husky": "^8.0.3",
		"jsonwebtoken": "^9.0.0",
		"patch-package": "^6.5.1",
		"postcss": "^8.4.21",
		"prettier": "^2.8.6",
		"prettier-plugin-svelte": "^2.9.0",
		"prettier-plugin-tailwindcss": "^0.2.5",
		"svelte": "^3.57.0",
		"svelte-check": "^3.1.4",
		"tailwindcss": "^3.2.7",
		"tslib": "^2.5.0",
		"typescript": "^5.0.2",
		"vite": "^4.2.1",
		"vitest": "^0.29.7"
	},
	"type": "module",
	"dependencies": {
		"@sveltejs/adapter-node": "^1.2.3",
		"chart.js": "^4.2.1",
		"chartjs-adapter-date-fns": "^3.0.0",
		"daisyui": "^2.51.5",
		"date-fns": "^2.29.3",
		"download-csv": "^1.1.1",
		"jwt-decode": "^3.1.2",
		"lodash.merge": "^4.6.2",
		"pg": "^8.10.0",
		"svelte-chartjs": "^3.1.2",
		"svelte-device-detector": "^0.2.2",
		"svelte-flatpickr": "^3.3.2",
		"uuid": "^9.0.0"
	}
}
